{
	"Dependencies": {
		"MySql": {
			"ConnectionString": "server=localhost;port=3306;database=TServiceNameT;user=TServiceNameT;password=TServiceNameT"
		}
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.Hosting.Lifetime": "Information",
				"Microsoft.AspNetCore.Hosting.Diagnostics": "Fatal"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3} {SourceContext}] {Message:lj} {NewLine} {Exception}"
				}
			},
			{
				"Name": "OpenTelemetry"
			}
		]
	}
}
