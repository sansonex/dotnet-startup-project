{
	"$schema": "http://json.schemastore.org/template",
	"author": "Sansone",
	"classifications": [
		"ASP.NET Core",
		"Core",
		"MVC",
		"Template",
		"Web"
	],
	"tags": {
		"language": "C#"
	},
	"identity": "Template",
	"name": ".NET Core Sansone Template",
	"shortName": "sansone-service",
	"sourceName": "DotNetCoreTemplate",
	"symbols": {
		"ServiceName": {
			"type": "parameter",
			"datatype": "string",
			"description": "The name of the service",
			"defaultValue": "WeatherForecast",
			"replaces": "TServiceNameT"
		}
	},
	"sources": [
		{
			"modifiers": [
				{
					"exclude": [
						"src/.vs/**",
						".template_config/**"
					]
				}
			]
		}
	]
}